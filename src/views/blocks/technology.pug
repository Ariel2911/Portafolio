- var oneTechItems = ['React', 'TypeScript', 'Styled-Component', 'JavaScript']
- var twoTechItems = ['HTML', 'CSS', 'Pug', 'Sass','NPM', 'Gulp', 'Webpack', 'Git', 'Figma']

section#technology.section
  h2.section__title Tecnologías
  .section__container
    ul.section__list
      each oneTechItem in oneTechItems
        li.card.card--technology
          img.card__img(src=`./assets/icons/${oneTechItem}.svg`, alt=`icono de ${oneTechItem}`)
          h3.card__title--technology=oneTechItem

    ul.section__list.more-technologies
      each twoTechItem in twoTechItems
        li.card.card--technology
          img.card__img(src=`./assets/icons/${twoTechItem}.svg`, alt=`icono de ${twoTechItem}`)
          h3.card__title--technology=twoTechItem    
           
    button#btnSeeMore.btn.btn-primary Ver más